{
  "version": 2,
  "builds": [{ "src": "server/server.js", "use": "@now/node-server" }],
  "routes": [
    {
      "src": "/users",
      "dest": "/server/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/users",
      "dest": "/server/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/users",
      "dest": "/server/server.js",
      "methods": ["PUT"]
    },
    {
      "src": "/users",
      "dest": "/server/server.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/",
      "dest": "/server/server.js",
      "methods": ["GET"]
    }
  ]
}